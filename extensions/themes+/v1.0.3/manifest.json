{
	"manifest_version": 3,
	"author": "buildwithkt.dev",
	"name": "Themes+ New Tab",
	"version": "1.0.3",
	"description": "Video wallpapers and customizable backgrounds for your new tab - plus tasks, notes, bookmarks, timers, and tab manager.",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"permissions": ["search", "storage", "tabs"],
	"optional_permissions": ["bookmarks", "alarms", "notifications"],
	"background": {
		"service_worker": "js/service.js",
		"type": "module"
	},
	"host_permissions": ["https://buildwithkt.dev/*"]
}
